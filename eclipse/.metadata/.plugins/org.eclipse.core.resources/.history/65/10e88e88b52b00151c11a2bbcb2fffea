package com.josephb.test1.block;

import com.josephb.test1.creativetab.CreativeTabTest1;

import net.minecraft.block.material.Material;

public class BlockMagnet extends BlockTest1
{
	private final String name = "Magnet";
	
	public BlockMagnet()
	{
		super(Material.iron);
		this.setUnlocalizedName(name);
		this.setCreativeTab(CreativeTabTest1.Test1_Tab);
	}
	
	@Override
	public String getName()
	{
		return name;
	}

}
